<style>
    .gage-transform-@(GageStyleId){
    transform: @(BaseTransform);
    }
</style>
@code {
    [Parameter]
    public string GageText { get; set; } = "";
    [Parameter]
    public string GageBottomText { get; set; } = "";
    [Parameter]
    public string BaseTransform { get; set; } = "";
    [Parameter]
    public string ID { get; set; } = "";


    public string GageStyleId { get; set; } = new Random().Next(1000, 9999).ToString();
    private int angle {
        get
        {
            var half = (max_range - min_range) / 2;            
            return (int)((Value - half) / (double)half * 125.0);
        }
    }
    private int min_range = 0;
    private int max_range = 6000;

    private string FormatDouble(double value)
    {
        return $"{value:0.#}";
    }

    private string GetLabel(int index)
    {
        var range = (To - From)/ 6;
        return index switch
        {
            0 => FormatDouble(From),
            6 => FormatDouble(To),
            _ => FormatDouble(From + index * range)
            };
    }
}