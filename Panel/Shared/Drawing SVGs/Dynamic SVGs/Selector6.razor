@code {
    private int[] SelectorAngles { get; set; } = new[] { 2, 51, 106, 180, 230, 288 };
    private string ClassId { get; set; } = new Random().Next(100, 999).ToString();
    [Parameter]
    public string BaseTransform { get; set; } = "";

    [Parameter]
    public int SelectNumber { get; set; } = 0;

    private int Angle { get; set; }

    protected override void OnInitialized()
    {
        CheckSelectNumberRange();
        base.OnInitialized();
    }

    private void ChangeAngle(int number)
    {
        Angle = SelectorAngles[number];
        StateHasChanged();
    }

    private void CheckSelectNumberRange()
    {

        if (SelectNumber > 4)
        {
            SelectNumber = 0;

            Angle = SelectorAngles[5];
        }
        else
        {
            Angle = SelectorAngles[SelectNumber];
            SelectNumber += 1;
        }
    }

}