@code {
    private BrowserDimension BrowserDimension { get; set; } = new();
    private double Width { get; set; }
    public int test_bind { get; set; }
    protected SimulationModel Simulation { set; get; } = new();
    protected string SimulationId { set; get; }

    private EditContext EditContext;
    private HubConnection? hubConnection;

        private const int chartMaxLength = 30;

    private ChartValueContainer pumpPressValues = new ChartValueContainer(chartMaxLength, "pump press");
    private ChartValueContainer cSGPressValues = new ChartValueContainer(chartMaxLength, "CSG press");
    private ChartValueContainer sPMValues = new ChartValueContainer(chartMaxLength, "SPM");
    private ChartValueContainer pitGainValues = new ChartValueContainer(chartMaxLength, "pit gain");


    protected async override void OnInitialized()
    {
        EditContext = new EditContext(Simulation);

        SimulationId = UriExtenssions.GetQueryStringValue(NavigationManager.Uri, "simulationId");
        Simulation = await SimulationApi.GetFromRedis(new Guid(SimulationId));
        StateHasChanged();
        EditContext.OnFieldChanged += OnFieldChanged;

        SetChockHandlerStatusFunc = async (i) =>
        {
            Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokeControlLever = i;
            var fieldIdentifier = FieldIdentifier.Create(() => Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokeControlLever);
            EditContext.NotifyFieldChanged(fieldIdentifier);

            Console.WriteLine($"Chokpanel Level set to: {i}");
        };
        
        
        await BrowserTools.UpdateChokeChart("chokeChart", new[]
        {
            pumpPressValues.Values,
            cSGPressValues.Values,
            sPMValues.Values,
            pitGainValues.Values
        },
            pumpPressValues.Indexes);

    #region hub comment

        hubConnection = (new HubConnectionBuilder().WithAutomaticReconnect().WithUrl($"{Statics.BaseAddress}SimulationHub")).Build();

        hubConnection.On<SimulationModel>("update-model", async (newModel) =>
        {
            if ((newModel as SimulationModel)?.Id != Guid.Parse(SimulationId))
                return;
            Lastupdate = DateTime.Now;

            Simulation.SimulationFeilds.InputValues = newModel.SimulationFeilds.InputValues;
            Simulation.SimulationFeilds.TempValues = newModel.SimulationFeilds.TempValues;
            Console.WriteLine("update-model");
            StateHasChanged();
        });
        hubConnection.On<SimulationModel>("Update-Outputs", async (newModel) =>
        {
            
            if ((newModel as SimulationModel)?.Id != Guid.Parse(SimulationId))
                return;
            Simulation.SimulationFeilds.OutputVals = newModel.SimulationFeilds.OutputVals;

    // pull up reset BTN when sim gets the signal
            if (Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokePanelStrokeResetSwitch && Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelStrokeResetSwitch)
                Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokePanelStrokeResetSwitch = false;
            
            cSGPressValues.AddPoint(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.CasingPressure);
            pumpPressValues.AddPoint(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.StandPipePressure);
            sPMValues.AddPoint(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelSPMCounter);
            pitGainValues.AddPoint(Simulation.SimulationFeilds.OutputVals.Equipments.DataDisplay.PitGainLossGauge);
            
            
            Console.WriteLine("Update-Outputs -- BLAZOR -- CHOKE PANEL");
            
            await BrowserTools.UpdateChokeChart("chokeChart", new[]
                {
                    pumpPressValues.Values,
                    cSGPressValues.Values,
                    sPMValues.Values,
                    pitGainValues.Values
                },
                pumpPressValues.Indexes);

    //set and update charts

    // BrowserTools.InitChockJs(cSGPressValues.Values, cSGPressValues.Indexes);
    // BrowserTools.InitChockJs(pumpPressValues.Values, pumpPressValues.Indexes);
    // BrowserTools.InitChockJs(sPMValues.Values, sPMValues.Indexes);
    // BrowserTools.InitChockJs(pitGainValues.Values, pitGainValues.Indexes);


            StateHasChanged();
        });


        await hubConnection.StartAsync();

    #endregion

        base.OnInitialized();
    }

    private DateTime Lastupdate { set; get; } = DateTime.Now;

    private async void OnFieldChanged(object sender, FieldChangedEventArgs e)
    {
        if (DateTime.Now.Subtract(Lastupdate).TotalMilliseconds < 100)
            return;
        Console.WriteLine("OnFieldChanged");
        if (hubConnection is not null && hubConnection.State == HubConnectionState.Connected)
        {
            await hubConnection.SendAsync("UpdateModel", Simulation);
        }
    }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await BrowserTools.InitChockJs();
        }

        await base.OnAfterRenderAsync(firstRender);
    }

    private void onValueChange()
    {
        StateHasChanged();
    }

    private string GetClass(double input, params double[] validArray)
    {
        return validArray.Any(x => Math.Abs(x - input) < 0.1 ) ? "active" : string.Empty;
    }

    private void PumpSwitchChanged(double input)
    {
        Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokePanelPumpSelectorSwitch = input;
        var fieldIdentifier = FieldIdentifier.Create(() => Simulation.SimulationFeilds.InputValues.Equipments.ChokeControl.ChokePanelPumpSelectorSwitch);
        EditContext.NotifyFieldChanged(fieldIdentifier);
