@page "/ChockPanel"
@layout SimulationLayout
@inject ISimulationApi SimulationApi
@using Microsoft.AspNetCore.SignalR.Client;
@using Models.Config;
@using Panel16.Util

<div id="chock-v2">
    <div class="top">
        <div class="controls">

            <div class="stop">
                <i class="bi bi-pause"></i>
            </div>
            <div class="end">
                <i class="bi bi-stop-fill"></i>
            </div>
        </div>

        <SpeedSelection/>

        <div class="timer">
            <div class="start">
                <span>R:</span> <span style="font-size: 0.8rem"></span>
            </div>
            <div class="end">
                <span>S:</span> <span style="font-size: 0.8rem"></span>
            </div>
        </div>
        <div class="warning click-scale-down" style="cursor: pointer">
            <i class="bi bi-exclamation-triangle"></i>
        </div>
        <div class="volume control click-scale-down" style="cursor: pointer;">

            <i class="bi bi-volume-mute"></i>


        </div>

        <div class="mic control click-scale-down">

            <i class="bi bi-mic"></i>

        </div>


        <div class="user">
            <a href="/SimulationUsers?simulationId=@(SimulationId)">
                <i class="bi bi-person"></i>
            </a>
        </div>


        <div class="chat">
            <a href="">
                <i class="bi bi-chat-left"></i>
            </a>
        </div>


        <div class="internet connect anim-blink pointer-event">
            <!--change connect to disconnect to make it red-->
            <i class="bi bi-circle-fill"></i>
        </div>

    </div>

    <div id="chock">
        <div class="charts">
            <div class="charts-preview">
                <canvas id="chokeChart"></canvas>
                @* <canvas id="myChart"></canvas> *@
            </div>
        </div>
        <div class="gages">
            <div class="top">
                <div class="gage gage-1">
                    <div class="system">
                        <img class="ceret" src="assets/img/ceret.png" alt="">
                        <div class="value segment">@($"{Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePosition:0}")</div>
                    </div>
                    <div class="drilling-label">Drill Pipe</div>
                </div>
                <div class="display">
                    <div class="name">SPM</div>
                    <div class="value segment">@($"{Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelSPMCounter}")</div>
                    <div class="name">Tot. Stk</div>
                    <div class="value segment">@($"{Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelTotalStrokeCounter:0}")</div>
                    <button @onclick="RestTotalStrokeClicked"
                            class="button">
                        Reset
                    </button>
                </div>
                <div class="gage gage-2">
                    <div class="system">
                        <img class="ceret" src="assets/img/ceret.png" alt="">
                        <div class="value segment">@($"{Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.CasingPressure:0}")</div>
                    </div>
                    <div class="drilling-label">Casing</div>
                </div>
            </div>

            <div class="middle">
                <div class="buttons">
                    <div
                        @onclick="Choke1Clicked"
                        class="btn btn-1 
@GetClass(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.Choke1LED, 1)">
                        Chock 1
                    </div>
                    <div
                        @onclick="Choke2Clicked"
                        class="btn btn-1
@GetClass(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.Choke2LED, 1)">
                        Chock 2
                    </div>
                </div>

                <div class="gage-detail">
                    <ul>
                        <li
                            @onclick="() => PumpSwitchChanged(1)"
                            class="@GetClass(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelPumpSelectorSwitch, 1)">
                            MP1
                        </li>
                        <li
                            @onclick="() => PumpSwitchChanged(3)"
                            class="@GetClass(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelPumpSelectorSwitch, 3)">
                            Both
                        </li>
                        <li
                            @onclick="() => PumpSwitchChanged(2)"
                            class="@GetClass(Simulation.SimulationFeilds.OutputVals.Equipments.ChokeControl.ChokePanelPumpSelectorSwitch, 2)">
                            MP2
                        </li>
                    </ul>
                </div>
                <div class="half-gage">
                    <img src="assets/img/half gage-01.png" alt="">
                    <img class="ceret" src="assets/img/ceret.png" alt="">
